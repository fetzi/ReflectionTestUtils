{
    "name": "jopic/reflectiontestutils",
    "description": "utility class for field injection in tests",
    "description": "Logging for PHP 5.3",
    "keywords": ["reflection","test utils"],
    "homepage": "https://github.com/fetzi/ReflectionTestUtils",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Johannes Pichler",
            "email": "admin@jopic.at",
            "homepage": "http://www.jopic.at",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*"
    },
    "autoload": {
        "psr-4": {
            "Jopic\\": "src/Jopic",
            "Jopic\\Tests\\": "tests",
            "Demo\\": "src/Demo"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "pre-commit":
        [
            "vendor/bin/phpunit"
        ]
    }
}
